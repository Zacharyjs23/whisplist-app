{
  "name": "whisplist",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "EXPO_ROUTER_APP_ROOT=app EXPO_ROUTER_TYPED_ROUTES=0 npx expo start",
    "android": "npx expo run:android",
    "ios": "LANG=en_US.UTF-8 LC_ALL=en_US.UTF-8 npx expo run:ios",
    "web": "EXPO_ROUTER_APP_ROOT=$PWD/app EXPO_ROUTER_IMPORT_MODE=sync EXPO_ROUTER_TYPED_ROUTES=0 npx expo start --web",
    "dev": "EXPO_ROUTER_APP_ROOT=app EXPO_ROUTER_TYPED_ROUTES=0 npx expo start --dev-client",
    "build:ios:dev": "LANG=en_US.UTF-8 LC_ALL=en_US.UTF-8 npx expo run:ios",
    "build:android:dev": "npx expo run:android",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "test": "jest",
    "test:rules": "jest -c jest.rules.config.js tests/rules/*.test.ts",
    "test:rules:emu": "FIRESTORE_EMULATOR_HOST=127.0.0.1:8080 jest -c jest.rules.config.js tests/rules/*.test.ts",
    "typecheck": "tsc --noEmit",
    "check:backend": "bash ./scripts/check-backend.sh",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\" \"!package-lock.json\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\" \"!package-lock.json\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-picker/picker": "^2.11.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "date-fns": "^2.29.3",
    "dotenv": "^16.5.0",
    "expo": "53.0.22",
    "expo-audio": "~0.4.9",
    "expo-auth-session": "~6.2.0",
    "expo-blur": "~14.1.5",
    "expo-clipboard": "^7.1.5",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-image-manipulator": "^14.0.7",
    "expo-image-picker": "~16.1.4",
    "expo-linking": "~7.1.5",
    "expo-localization": "^16.1.6",
    "expo-notifications": "~0.31.3",
    "expo-router": "~5.1.5",
    "expo-splash-screen": "~0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "firebase": "^11.10.0",
    "i18next": "^25.3.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^15.6.1",
    "react-native": "0.79.6",
    "react-native-chart-kit": "^6.12.0",
    "react-native-confetti-cannon": "^1.5.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-purchases": "^9.5.1",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/ngrok": "^4.1.3",
    "@firebase/rules-unit-testing": "^4.0.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "firebase-tools": "^14.15.2",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.10",
    "prettier": "^3.6.2",
    "react-devtools": "^6.1.5",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.8.3"
  },
  "overrides": {
    "superstatic": "10.0.0"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "<rootDir>/tests/setupEnv.ts"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "<rootDir>/tests/setupLogger.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/tests/rules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    }
  },
  "private": true
}
